    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "socket.io-redis": "^6.1.1",
    "typeorm": "^0.3.27"
