  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/microservice-nofitication/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "docker-compose --project-directory ./e2e up e2e",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "pnpm typeorm migration:generate -d ormconfig.ts",
    "migration:create": "pnpm typeorm migration:create",
    "migration:run": "pnpm typeorm migration:run -d ormconfig.ts",
    "migration:revert": "pnpm typeorm migration:revert -d ormconfig.ts",
    "migration:show": "pnpm typeorm migration:show -d ormconfig.ts"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@bull-board/nestjs": "^6.13.0",
